<h1>News</h1>

<table class="table table-bordered table-striped">
  <thead>
    <tr>
      <th>Title</th>
      <th>Content</th>
    </tr>
  </thead>
  <tbody>
    <% for (var post in news) { %>
      <tr>
        <td><%= news[post].title %></td>
        <td><%- news[post].content %></td>
      </tr>
    <% } %>
  </tbody>
</table>

<%- linkTo("Add Post", addNewsPath, { class: "btn btn-primary" }); %>
